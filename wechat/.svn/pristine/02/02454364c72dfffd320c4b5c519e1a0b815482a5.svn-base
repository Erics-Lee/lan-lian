/*maskBox*/
#maskBox{
	width: 100%;
	height: 100%;
	background: rgba(92,90,90,0.4);
	position: absolute;
	top: 0;
	left: 0;
	display: none;
}
#maskBox .emailBox,#maskBox .phoneBox,#maskBox .componyBox,#maskBox .phoneBox1,#maskBox .phoneBox2{
	min-width: 300px;
	height: 210px;
	position: absolute;
	top: 50%;
	margin-top: -125px;
	left: 50%;
	margin-left: -150px;
	background-color: #fff;
	border-radius: 5px;
	display: none;
}

