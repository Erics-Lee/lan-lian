	font-size: 16px;
	color: #000;
	height:32px;
	line-height: 32px;
	margin-top: 20px;
}
#maskBox .phoneBox2 .email .maskBig{
	font-size: 16px;
	color: #000;
	height:32px;
	line-height: 32px;
	margin-top: 30px;
}
