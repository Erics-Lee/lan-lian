*{
	margin: 0;
	padding: 0;
	font-size: 12px;
}
ul li{
	margin: 0;
	padding: 0;
	list-style: none;
}
html,body{
	width: 100%;
	height: 100%;
	background-color: #fff;
}
body {
　　　　-webkit-overflow-scrolling: touch;
　　　　overflow-scrolling: touch;
　　}
#wrap{
	width: 100%;
	height: 100%;
	display: flex;
	flex-direction: column;
}
#wrap .header{
	width: 100%;
	height: 183px;
	position: relative;
	flex: 0 0 auto;
}
#wrap .header .headerBox{
	width: 100%;
	height: 150px;
}
#wrap .header .headerBox .banner{
	width: 100%;
	height: 150px;
	position: absolute;
	top: 0;
	left: 0;
}
#wrap .header .headerBox .banner img{
	width: 100%;
	height: 100%;
}
#wrap .header .headerBox ul{
	padding: 0;
	list-style: none;
	height: 4px;
	position: absolute;
	bottom: 45px;
	left: 50%;
	margin-left: -25px;
}
#wrap .header .headerBox ul li{
	padding: 0;
	list-style: none;
	width: 50px;
	height: 2px;
	background-color:#12aaeb;
	float: left;
}

/*tongZhi*/
#wrap .header .tongZhi{
	width: 100%;
	height: 33px;
	background-color: #12aaeb;
	line-height: 33px;
	color: #fff;
}
#wrap .header .tongZhi img{
	display:block;
	width: 18px;
	height: 15px;
	margin: 9px 8px 0 13px;
	float: left;
}
#wrap .header .tongZhi span{
	display: block;
	float: left;
}

/*content*/
#wrap .content{
	width: 100%;
	background-color: #5682b2;
	height: 59%;
	overflow: auto;
	-webkit-overflow-scrolling: touch;
	flex: 1 0 auto;
}
#wrap .content .middle{
	width: 100%;
}
#wrap .content .middleTop{
	width: 100%;
	height:110px;
}
#wrap .content .middleTop .middleTop1,#wrap .content .middleTop .middleTop2{
	float: left;
	margin-top: 20px;
}
#wrap .content .middleTop .middleTop1{
	width: 42%;
