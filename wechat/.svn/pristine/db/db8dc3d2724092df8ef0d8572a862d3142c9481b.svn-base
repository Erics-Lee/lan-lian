* {
	margin: 0;
	padding: 0;
	font-size: 13px;
	/*color: #1a1a1a;*/
}

ul li {
	margin: 0;
	padding: 0;
	list-style: none;
}

html,
body {
	width: 100%;
	height: 100%;
	background-color: #fff;
}

body {
　　　-webkit-overflow-scrolling: touch;
　　　overflow-scrolling: touch;
}
p{
	white-space:nowrap; 
	text-overflow:ellipsis; 
	overflow:hidden;
}
#tax {
	width: 100%;
	height: 100%;
	display: flex;
	flex-direction: column;
}

#tax .header {
	width: 100%;
	height: 136px;
	background-image: linear-gradient(to right, #5682b2, #12aaeb);
	flex:0 0 auto;
}

#tax .content {
	width: 94%;
	margin-left: 3%;
	margin-top: -120px;
	height: 80%;
	background-color: #fff;
	/*border-radius: 5px;*/
	overflow: auto;
	-webkit-overflow-scrolling: touch;
	flex: 1 0 auto;
}

#tax .content .taxStep {
	width: 100%;
	/*border-radius: 10px;*/
	margin-bottom: 26px;
	/*border-top-left-radius: 5px;
	border-top-right-radius: 5px;*/
}

#tax .content .taxStep ul {
	width: 100%;
	height: 45px;
	background-image: linear-gradient(to right, #5682b2, #12aaeb);
	/*border-radius: 5px;*/
	/*border-top-left-radius: 5px;
	border-top-right-radius: 5px;*/
}

#tax .content .taxStep ul li {
	width: 25%;
	height: 35px;
	line-height: 35px;
	background-color: #d7d7d7;
	float: left;
	position: relative;
	margin-top: 10px;
}
#tax .content .taxStep ul li .taxMask1,#tax .content .taxStep ul li .taxMask2,#tax .content .taxStep ul li .taxMask3,#tax .content .taxStep ul li .taxMask4{
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
	display: none;
}
#tax .content .taxStep ul li:first-child{
	border-top-left-radius: 5px;
}
#tax .content .taxStep ul li:last-child{
	border-top-right-radius: 5px;
}
#tax .content .taxStep ul .active3 {
	width: 25%;
	height: 40px;
	line-height: 40px;
	background-color: #fff;
	color: #12AAEB;
	float: left;
	position: relative;
	margin-top: 5px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	
}

#tax .content .taxStep ul .active5 {
	width: 25%;
	height: 36px;
	line-height: 36px;
	background-color: #fff;
	color: #5682b2;
	float: left;
	position: relative;
	border-top-left-radius: 10px;
}

#tax .content .taxStep ul li img,
#tax .content .taxStep ul li input {
	display: block;
	float: left;
}

#tax .content .taxStep ul li input {
	min-width: 45px;
	overflow: hidden;
	text-overflow: ellipsis;
	border: none;
	outline: none;
	background: none;
	height: 36px;
	/*line-height: 36px;*/
	font-size: 13px;
	font-weight: bold;
	color: #6b6d6e;
	margin-top: -2px;
	position: relative;
}

#tax .content .taxStep ul li img {
	width: 16px;
	height: 17px;
	margin: 8px 5px 0 8px;
}

#tax .content .taxStep ul li .taxStepBottom {
	width: 20px;
	height: 2px;
	background-color: #12AAEB;
	position: absolute;
	bottom: 0;
	left: 50%;
	margin-left: -10px;
	display: none;
}


