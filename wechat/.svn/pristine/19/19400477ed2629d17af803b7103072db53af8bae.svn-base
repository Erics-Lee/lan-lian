function getDataAJAX1(url, type, data, async, callback) {
	var obj = data;
	$.ajax({
		type : type,
		url : url + "?_=" + Date.parse(new Date()) / 1000,
		async : async,
		data : obj,
		datatype : "text",
		beforeSend:function(){
			$("#loading").css("display","block")
		},
		success : function(res) {
			callback(res);
		},
		complete:function(){
			$("#loading").css("display","none")
		},
		error : function(e) {
		}
	});
};